{
    "manifest_version": 3,
    "name": "chrome-image-blocker-youtube-demo",
    "version": "1.0",
    "description": "A simple extension to toggle a Chrome setting.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": ["storage", "contentSettings", "tabs"],
    "background": {
        "service_worker": "background.js"
    }
  }
  